{
  "name": "reactblog",
  "version": "0.1.0",
  "description": "React blog",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "serve:prod": "webpack-dev-server --config config/webpack.prod.js --progress --colors --display-error-details",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached --inline --open",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --progress --colors --display-error-details",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxcache/reactblog.git"
  },
  "keywords": [
    "reactjs",
    "blog"
  ],
  "author": "Vikram",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxcache/reactblog/issues"
  },
  "homepage": "https://github.com/maxcache/reactblog#readme",
  "dependencies": {
    
  },
  "devDependencies": { 
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "ts-loader": "^1.2.2",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "^0.17.0"
  }
}
