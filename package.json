{
  "name": "fireblog",
  "version": "0.1.0",
  "description": "Firebase blog",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "serve:prod": "webpack-dev-server --config config/webpack.prod.js --progress --colors --display-error-details",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached --inline --open",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --progress --colors --display-error-details",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxcache/fireblog.git"
  },
  "keywords": [
    "firebase",
    "blog"
  ],
  "author": "Vikram",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxcache/fireblog/issues"
  },
  "homepage": "https://github.com/maxcache/fireblog#readme",
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "ts-loader": "^1.2.2",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "^0.17.0"
  }
}
